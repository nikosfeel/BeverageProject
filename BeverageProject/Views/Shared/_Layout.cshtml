<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title  E-Beverage</title>
    @Scripts.Render("~/bundles/modernizr")
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Beer, Spirits, Wine, Whiskey">
    <meta name="keywords" content="Beer, Spirits, Wine, Whiskey">
    <meta name="author" content="E-Beverage Team">
    <link rel="shortcut icon" href="~/Content/images/wine-glass.png" type="image/png" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/css/animate.min.css" rel="stylesheet">
    <link href="~/Content/css/jquery.bxslider.css" rel="stylesheet">
    <link href="~/Content/css/owl.carousel.css" rel="stylesheet">
    <link href="~/Content/css/paira.css" rel="stylesheet">
    <link href="~/Content/css/paira-color-font.css" rel="stylesheet">
    <link href="~/Content/css/paira-responsive.css" rel="stylesheet">
    <link href="~/Content/css/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/css/css/FONTS.css" rel="stylesheet" />
    <link href="~/Content/SortByStyle.css" rel="stylesheet" />
    <link href="~/Content/chat-and-backToTop.css" rel="stylesheet" />
    <link href="~/vue/dist/main.css" rel="stylesheet" />
    

</head>
<body>
    <div class="paira-container">
        @{
            Html.RenderAction("NavBarCategories", "NavigationBar");
        }
        @Html.Partial("~/Views/Shared/ClientComponents/_CartView.cshtml")

        @RenderBody()

        <hr />
        <footer>
            @Html.Partial("~/Views/Shared/ClientComponents/_Footer.cshtml")
        </footer>
    </div>
    <button id="backToTop" class="fa fa-arrow-up fa-2x"></button>
    <a href=@Url.Action("Chat", "Home") class="chat fa fa-commenting-o fa-3x"></a>

    <script src="~/Scripts/BackToTopButton.js"></script>
    <script src="~/Scripts/jquery-3.2.1.js"></script>
    <script src="~/Scripts/jquery-migrate-3.0.0.min.js"></script>
    <script src="~/Scripts/jquery.easing.1.3.js"></script>
    <script src="~/Scripts/jquery.waypoints.min.js"></script>
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <script src="~/Scripts/owl.carousel.min.js"></script>
    <script src="~/Scripts/jquery.bxslider.min.js"></script>
    <script src="~/Scripts/jquery.mousewheel.min.js"></script>
    <script src="~/Scripts/jquery.parallax-1.1.3.js" type="text/javascript"></script>
    <script src="~/Scripts/paira.js"></script>
    <script src="~/vue/dist/main.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>