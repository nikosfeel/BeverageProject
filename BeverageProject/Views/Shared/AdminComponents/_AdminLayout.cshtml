<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="ThemeBucket">
    <link rel="shortcut icon" href="images/favicon.png">

    <title></title>

    <!--Core CSS -->
    <link href="~/Scripts/AdminJs/bs3/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap-reset.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome/css/font-awesome.css" rel="stylesheet" />

    <!--external css-->
    <link href="~/Scripts/AdminJs/gritter/css/jquery.gritter.css" rel="stylesheet" />

    <!-- Custom styles for this template -->
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link href="~/Content/css/style-responsive.css" rel="stylesheet" />

    <!-- Just for debugging purposes. Don't actually copy this line! -->
    <!--[if lt IE 9]>
    <script src="js/ie8-responsive-file-warning.js"></script><![endif]-->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <section id="container">
        <!--Navigation start-->
        @Html.Partial("~/Views/Shared/AdminComponents/_NavigationBar.cshtml")
        <!--Navigation end-->
        <!-- sidebar menu start-->
        @Html.Partial("~/Views/Shared/AdminComponents/_SideBar.cshtml")
        <!-- sidebar menu end-->
        <!--main content start-->
        @RenderBody()
        <!--main content end-->
        <!--right sidebar start-->
        @Html.Partial("~/Views/Shared/AdminComponents/_RightSideBar.cshtml")
        <!--right sidebar end-->

    </section>
    @RenderSection("scripts", required: false)
    <!-- Placed js at the end of the document so the pages load faster -->
    <!--Core js-->
    <script src="~/Scripts/AdminJs/jquery.js"></script>
    <script src="~/Scripts/AdminJs/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/AdminJs/bs3/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/Scripts/AdminJs/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Scripts/AdminJs/jquery.scrollTo.min.js"></script>
    <script src="~/Scripts/AdminJs/jQuery-slimScroll-1.3.0/jquery.slimscroll.js"></script>
    <script src="~/Scripts/AdminJs/jquery.nicescroll.js"></script>
    <script type="text/javascript" src="js/gritter/js/jquery.gritter.js"></script>
    <!--Easy Pie Chart-->
    <script src="~/Scripts/AdminJs/easypiechart/jquery.easypiechart.js"></script>
    <!--Sparkline Chart-->
    <script src="~/Scripts/AdminJs/sparkline/jquery.sparkline.js"></script>
    <!--jQuery Flot Chart-->
    <script src="~/Scripts/AdminJs/flot-chart/jquery.flot.js"></script>
    <script src="~/Scripts/AdminJs/flot-chart/jquery.flot.tooltip.min.js"></script>
    <script src="~/Scripts/AdminJs/flot-chart/jquery.flot.resize.js"></script>
    <script src="~/Scripts/AdminJs/flot-chart/jquery.flot.pie.resize.js"></script>
    <!--Chart JS-->
    <script src="~/Scripts/AdminJs/chart-js/Chart.js"></script>
    <script src="~/Scripts/AdminJs/chartjs.init.js"></script>


    <!--BUG-->
    @*<script src="~/Scripts/AdminJs/dashboard.js"></script>*@
    <!--BUG-->



    <!--common script init for all pages-->
    <script src="~/Scripts/AdminJs/scripts.js"></script>

    <!--script for this page-->
    <script src="~/Scripts/AdminJs/gritter.js" type="text/javascript"></script>

</body>
</html>
