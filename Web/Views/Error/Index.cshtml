@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<!--=================== Main Content Container ===================-->
<main class="error-page">
    <!--=================== Breadcrumb Section ===================-->
    <section class="breadcrumb-container paira-padding-bottom-1">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <div class="breadcrumb">
                        <ul class="list-inline">
                            <li><a href="index.html" class="raleway-light">Home <i class="fa fa-angle-right"></i></a></li>
                            <li class="raleway-sbold">Page not found</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--=================== Error Section ===================-->
    <section class="error text-center paira-padding-bottom-1">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <img alt="" class="paira-margin-bottom-1" src="images/hedding.png">
                    <h3 class="text-uppercase raleway-light margin-clear paira-margin-bottom-1">Page not found</h3>
                </div>
                <div class="col-sm-12 col-md-12 col-xs-12 paira-margin-top-1">
                    <a href="#">
                        <img src="images/error.png" alt="" class="img-responsive">
                    </a>
                    <h3 class="raleway-sbold text-capitalize paira-margin-top-3">Sorry your page is not found</h3>
                    <h4 class="margin-top-15"><a href="index.html" class="raleway-light">Please go back</a></h4>
                    <a href="/" class="btn btn-default btn-lg color-scheme-2 forget-password text-uppercase paira-margin-top-1">Go to home page</a>
                    
                </div>
            </div>
        </div>
    </section>
</main>